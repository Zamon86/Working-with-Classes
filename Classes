public class Main {
    public static void main(String[] args) {
        int[] tab = {1,2,3,4,5,6,7};
        int[] tab2 = {1,2,3,4,5,6};
        Q1 sum = new Q1(tab);
        Q1 sum2 = new Q1(tab2);

        System.out.println(sum.sum);
        System.out.println(sum2.sum);
    }
}

public class Q1 {
    int sum;

    public Q1() {
    }

    public Q1(int[] tab){
        for (int i = 0; i < tab.length; i++) {
            sum = sum + tab[i];
        }
    }
}
